<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <EnableWindowsTargeting>true</EnableWindowsTargeting>
    <Nullable>enable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Icons\AlignCenter.png" />
    <None Remove="Icons\AlignJustify.png" />
    <None Remove="Icons\AlignLeft.png" />
    <None Remove="Icons\AlignRight.png" />
    <None Remove="Icons\NumberedList.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Xceed.Wpf.Toolkit" Version="4.5.2" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Icons\AlignCenter.png" />
    <Resource Include="Icons\AlignJustify.png" />
    <Resource Include="Icons\AlignLeft.png" />
    <Resource Include="Icons\AlignRight.png" />
    <Resource Include="Icons\NumberedList.png" />
  </ItemGroup>
</Project>
